{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "jsx-a11y/href-no-hash": "off",
    "react/jsx-filename-extension": "off",
    "react/no-array-index-key": "off"
  },
  "env": {
    "browser": true
  }
}
